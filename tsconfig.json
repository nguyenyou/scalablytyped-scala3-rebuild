{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "jsx": "react-jsx",
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "sourceMap": true,
    "paths": {
			"*": ["./cli-ts/*"],
			"@/*": ["./cli-ts/src/*"]
		}
  },
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ]
}