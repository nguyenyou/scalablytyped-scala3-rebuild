{
  "name": "scalablytyped-rebuild",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "generate": "rm -rf generated-sources && ./mill cli.runMain org.scalablytyped.converter.cli.Tracing",
    "cli:fix": "./mill cli.fix",
    "cli:fmt": "./mill mill.scalalib.scalafmt.ScalafmtModule/ cli.sources",
    "test:ts": "bun test",
    "test:scala": "./mill cli.test",
    "coverage:scala": "./mill cli.scoverage.htmlReport && bunx --bun serve@latest out/cli/scoverage/htmlReport.dest",
    "coverage:ts": "bun test --coverage",
    "test:all": "concurrently \"npm run test:ts\" \"npm run test:scala\""
  },
  "devDependencies": {
    "@types/bun": "latest",
    "concurrently": "9.2.1",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@types/tabulator-tables": "6.2.10",
    "clsx": "2.1.1",
    "scroll-into-view-if-needed": "3.1.0",
    "tabulator-tables": "6.3.1"
  }
}